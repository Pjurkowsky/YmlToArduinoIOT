board:
    platform: arduino:avr
    type: uno
    port: /dev/ttyUSB0
inputs:
    - name: led_1
      mode: ANALOG
      type: SENSOR
      source: 20
    - name: asdsad
      mode: ANALOG
      type: SENSOR
      source: 22
outputs:
    - name: buzzer_1
      mode: ANALOG
      pin: 11
constants:
    - name: pi
      value: 3.14
signals:
    - name: touch_1_pressed
      expression: 4 > 1
rules:
    - if: touch_1_pressed
      then: SET led_1 ON
    - if: !touch_1_pressed
      then: SET led_1 OFF
